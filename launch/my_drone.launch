<launch>
  <node pkg="tf2_ros" type="static_transform_publisher" name="UAV_camera_tf_publisher" args="0 0 0 -1.5707 0 -1.5707 UAV camera_frame" />
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find my_drone)/worlds/my_drone.world"/>
    <!-- more default parameters can be changed here -->
  </include>
</launch>
